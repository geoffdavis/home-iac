# 1Password Account Configuration
OP_ACCOUNT=YourAccountName

# 1Password Item References for AWS Credentials
# Update these to match your 1Password item structure
OP_AWS_VAULT=Private
OP_AWS_ITEM="AWS Access Key - S3 - Personal"
OP_AWS_ACCESS_KEY_FIELD="access key id"
OP_AWS_SECRET_KEY_FIELD="secret access key"

# For items with sections, specify the section reference
OP_AWS_SECTION=

# GitHub Token Configuration for Renovate
# Update these to match your 1Password item structure
GITHUB_TOKEN_VAULT=Private
GITHUB_TOKEN_ITEM="GitHub Personal Access Token"
GITHUB_TOKEN_FIELD=token
